// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model student {
  id               Int       @id @default(autoincrement())
  email            String
  studentName      String?
  fatherName       String?
  dob              String?
  passedGrade      String?
  gradeToAttend    String?
  campusTime       String?
  intakes          String?
  oldSchoolName    String?
  city             String?
  country          String
  phoneNumber      String?
  residentialAddress String?
  createdAt        Float
  token            String
  url       String
}
